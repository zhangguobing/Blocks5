<?xml version="1.0" encoding="utf-8"?>
<resources>
    <declare-styleable name="BlocksEditText">
        <attr name="iconClear" format="reference" />
    </declare-styleable>

    <declare-styleable name="MultiStateView">
        <attr name="msv_loadingView" format="reference" />
        <attr name="msv_errorView" format="reference" />
        <attr name="msv_emptyView" format="reference" />
        <attr name="msv_viewState" format="enum">
            <enum name="content" value="0" />
            <enum name="loading" value="1" />
            <enum name="error" value="2" />
            <enum name="empty" value="3" />
        </attr>
    </declare-styleable>

    <declare-styleable name="SlidingMenu">
        <attr name="mode">
            <enum name="left" value="0" />
            <enum name="right" value="1" />
        </attr>
        <attr name="viewAbove" format="reference" />
        <attr name="viewBehind" format="reference" />
        <attr name="behindOffset" format="dimension" />
        <attr name="behindWidth" format="dimension" />
        <attr name="behindScrollScale" format="float" />
        <attr name="touchModeAbove">
            <enum name="margin" value="0" />
            <enum name="fullscreen" value="1" />
            <enum name="none" value="2" />
        </attr>
        <attr name="touchModeBehind">
            <enum name="margin" value="0" />
            <enum name="fullscreen" value="1" />
            <enum name="none" value="2" />
        </attr>
        <attr name="shadowDrawable" format="reference" />
        <attr name="shadowWidth" format="dimension" />
        <attr name="fadeEnabled" format="boolean" />
        <attr name="fadeDegree" format="float" />
        <attr name="selectorEnabled" format="boolean" />
        <attr name="selectorDrawable" format="reference" />
    </declare-styleable>

    <declare-styleable name="DanmakuView">
        <attr name="max_row" format="integer" />
        <attr name="pick_interval" format="integer" />
        <attr name="max_running_per_row" format="integer" />
        <attr name="show_debug" format="boolean" />
        <attr name="start_Y_offset" format="float" />
        <attr name="end_Y_offset" format="float" />
    </declare-styleable>

    <declare-styleable name="DropDownView">
        <attr name="containerBackgroundColor" format="color" />
        <attr name="overlayColor" format="color"/>
    </declare-styleable>

    <declare-styleable name="FlowRadioGroup">
        <attr name="radio_columu_num" format="integer" />
    </declare-styleable>

    <declare-styleable name="password_edit_text">
        <attr name="show_drawable" format="integer"/>
        <attr name="hide_drawable" format="integer"/>
        <attr name="password_visible" format="boolean"/>
    </declare-styleable>

</resources>
